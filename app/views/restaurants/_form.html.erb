<div class="container">

  <%= simple_form_for @restaurant do |f| %>
    <%= f.error_notification %>

    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :phone_number %>
    <%= f.input :category %>

  <div class="text-center">
      <%= f.button :submit, class: "btn btn-primary btn-head" %>
    <% end %>
  </div>
</div>
